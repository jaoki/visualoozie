<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="net.junaoki.lpbw.hibernate.EventPlanPlanner" table="event_plan_planner" catalog="lpbw">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>

		<many-to-one name="eventPlanId" column="EVENT_PLAN_ID" class="net.junaoki.lpbw.hibernate.EventPlan" not-null="true" cascade="none"/>

        <property name="planner_id" type="string">
            <column name="PLANNER_ID" length="20" not-null="true" unique="false" />
        </property>

    </class>
</hibernate-mapping>




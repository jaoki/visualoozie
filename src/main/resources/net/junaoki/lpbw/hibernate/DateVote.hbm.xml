<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="net.junaoki.lpbw.hibernate.DateVote" table="date_vote" catalog="lpbw">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>

		<many-to-one 
			name="eventPlanDateId" 
			column="EVENT_PLAN_DATE_ID" 
			class="net.junaoki.lpbw.hibernate.EventPlanDate" 
			not-null="true" 
			cascade="none"/>

        <property name="vote">
        	<type name="org.hibernate.type.EnumType">
        		<param name="enumClass">net.junaoki.lpbw.VoteStatusEnum</param>
				<param name="type">12</param> <!-- java.sql.Types  public static final int VARCHAR = 12; -->
        	</type>
        </property>

    </class>
</hibernate-mapping>



